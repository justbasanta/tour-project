        </activity>
        <activity
            android:name="com.example.jitu516.tournepal.Explore"
            android:label="@string/title_activity_explore" >
            <intent-filter>
                <action android:name="com.example.jitu516.tournepal.Explore" />

                <category android:name="android.intent.category.DEFAULT" />
