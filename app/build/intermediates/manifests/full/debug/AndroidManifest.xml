            android:label="@string/title_activity_tour" >
            <intent-filter>
                <action android:name="com.example.jitu516.tournepal.Tour" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>